<!DOCTYPE hibernate-mapping PUBLIC 
"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd"> 
    
<hibernate-mapping default-lazy="false">
<!-- 
    Created by the Middlegen Hibernate plugin 2.1

    http://boss.bekk.no/boss/middlegen/
    http://www.hibernate.org/
-->

<class 
    name="gov.nih.nci.security.authorization.domainobjects.Group" 
    table="CSM_GROUP"
>

    <id
        name="groupId"
        type="java.lang.Long"
        column="GROUP_ID"
        unsaved-value="0"
    >
        <generator class="native">
		 <param name="sequence">CSM_GROUP_GROUP_ID_SEQ</param>
	 </generator>
    </id>

    <property
        name="groupName"
        type="java.lang.String"
        column="GROUP_NAME"
        length="255"
    />
    <property
        name="groupDesc"
        type="java.lang.String"
        column="GROUP_DESC"
        length="200"
    />
    <property
        name="updateDate"
        type="java.sql.Date"
        column="UPDATE_DATE"
        length="10"
    />

    <!-- Associations -->
  
    
    <many-to-one
        name="application"
        class="gov.nih.nci.security.authorization.domainobjects.Application"
        not-null="true"
    >
        <column name="APPLICATION_ID" />
    </many-to-one>
    
    <set name="users" lazy="true" table="CSM_USER_GROUP">
           <key column="group_id"/>
           <many-to-many column="user_id" class="gov.nih.nci.security.authorization.domainobjects.User"/>
     </set>

</class>
</hibernate-mapping>
